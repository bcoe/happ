{
  "name": "happ",
  "version": "1.0.0",
  "description": "",
  "main": "index.mjs",
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "fix": "eslint '**/*.mjs' '**/*.cjs' --fix",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "c8 cross-env NODE_ENV=test mocha ./test/api.mjs",
    "test:frontend": "vitest --coverage --run ./app",
    "posttest": "eslint '**/*.mjs' '**/*.cjs'",
    "start": "node --import=./lib/sentry.mjs ./lib/index.mjs",
    "consume": "node lib/worker.mjs start",
    "start:dev": "concurrently 'npm run start' 'npm run consume'",
    "db:up": "knex migrate:latest --knexfile=knexfile.cjs",
    "db:rollback": "knex migrate:rollback --knexfile=knexfile.cjs",
    "db:create-migration": "knex migrate:make --knexfile=knexfile.cjs"
  },
  "author": "Ben Coe <bencoe@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@remix-run/express": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@sentry/core": "^8.14.0",
    "@sentry/node": "^8.14.0",
    "@sentry/profiling-node": "^8.14.0",
    "@sentry/remix": "^8.14.0",
    "@sentry/utils": "^8.14.0",
    "@sentry/vite-plugin": "^2.20.1",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "google-auth-library": "^9.7.0",
    "isbot": "^4.4.0",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "recharts": "^2.12.6",
    "redis": "^4.6.13",
    "remix": "^2.8.1",
    "remix-utils": "^7.6.0",
    "vite": "^5.2.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@remix-run/testing": "^2.8.1",
    "@testing-library/react": "^15.0.2",
    "@types/react": "^18.2.77",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.5.0",
    "autoprefixer": "^10.4.19",
    "c8": "^9.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jsdom": "^24.0.0",
    "mocha": "^10.3.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "vitest": "^1.5.0"
  }
}
